      }
    },
    "registration-e2e": {
      "root": "apps/registration-e2e",
      "sourceRoot": "apps/registration-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
